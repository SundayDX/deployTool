        "ssh": {
            "enabled": true,
            "host": "192.168.1.100",
            "port": 22,
            "user": "root",
            "key_file": "/root/.ssh/id_rsa"
        }
    },
    {
        "name": "SSH密码认证示例",
        "description": "使用密码认证的SSH项目（不推荐）",
        "path": "/home/user/project",
        "auto_restart": true,
        "ssh": {
            "enabled": true,
            "host": "example.com",
            "port": 22,
            "user": "deploy",
            "password": "your_password_here"
        }
    }
]
