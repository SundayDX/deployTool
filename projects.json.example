[
    {
        "name": "本地项目示例",
        "description": "在本地服务器上运行的项目",
        "path": "/path/to/your/project1",
        "auto_restart": true
    },
    {
        "name": "远程SSH项目示例",
        "description": "通过SSH管理的远程项目",
        "path": "/path/to/remote/project",
